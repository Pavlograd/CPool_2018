** File description:
** task03
*/

#include <stdio.h>
#include <unistd.h>

void my_putchar(char c)
{
    write(1, &c, 1);
}

int my_strlen(char const *str)
{
    int len = sizeof(*str);
    
    my_putchar(len);
}

int main(void)
{
    char str = "hello";
    my_strlen(&str);
    return (0);
}
